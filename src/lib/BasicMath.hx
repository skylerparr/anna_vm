package lib;
class BasicMath {
  public function new() {
  }

  public static function add(left: Int, right: Int): Int {
    return left + right;
  }

  public static function subtract(left: Int, right: Int): Int {
    return left - right;
  }

  public static function multiply(left: Int, right: Int): Int {
    return left * right;
  }

  public static function divide(left: Int, right: Int): Float {
    return left / right;
  }
}
